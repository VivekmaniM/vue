n          ></v-checkbox>\r\n          <v-checkbox\r\n            v-model=\"bottom\"\r\n            label=\"Bottom\"\r\n            hide-details\r\n          ></v-checkbox>\r\n          <v-checkbox\r\n            v-model=\"left\"\r\n            label=\"Left\"\r\n            hide-details\r\n          ></v-checkbox>\r\n        </v-col>\r\n        <v-col\r\n          cols=\"12\"\r\n          sm=\"6\"\r\n          md=\"4\"\r\n        >\r\n          <v-subheader>Speed dial direction</v-subheader>\r\n          <v-radio-group\r\n            v-model=\"direction\"\r\n            hide-details\r\n          >\r\n            <v-radio\r\n              value=\"top\"\r\n              label=\"Top\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"right\"\r\n              label=\"Right\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"bottom\"\r\n              label=\"Bottom\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"left\"\r\n              label=\"Left\"\r\n            ></v-radio>\r\n          </v-radio-group>\r\n        </v-col>\r\n        <v-col\r\n          cols=\"12\"\r\n          sm=\"6\"\r\n          md=\"4\"\r\n        >\r\n          <v-subheader>Transition</v-subheader>\r\n          <v-radio-group\r\n            v-model=\"transition\"\r\n            hide-details\r\n          >\r\n            <v-radio\r\n              value=\"slide-y-transition\"\r\n              label=\"Slide y\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"slide-y-reverse-transition\"\r\n              label=\"Slide y reverse\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"slide-x-transition\"\r\n              label=\"Slide x\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"slide-x-reverse-transition\"\r\n              label=\"Slide x reverse\"\r\n            ></v-radio>\r\n            <v-radio\r\n              value=\"scale-transi