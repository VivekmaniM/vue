ndeterminate\n      ></v-progress-linear>\n    </template>\n          <v-col\n            lg=\"12\"\n            sm=\"12\"\n            md=\"12\"\n            class=\"logo\"\n          >\n            <v-img src=\"../assets/qwiktime.png\"/>\n        </v-col>\n        <v-card-title>Log In</v-card-title>\n        <v-card-text>\n          <div>Telliant Timesheet Application</div>\n        </v-card-text>\n        <v-col\n          lg=\"12\"\n          sm=\"6\"\n          md=\"3\"\n        >\n          <v-text-field\n            label=\"Email Address\"\n            outlined\n            v-model=\"loginEmail\"\n          ></v-text-field>\n        </v-col>\n        <v-col>\n          <v-text-field\n            v-model=\"loginPassword\"\n            :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n            :rules=\"[rules.required, rules.min]\"\n            :type=\"show ? 'text' : 'password'\"\n            name=\"input-10-1\"\n            label=\"Password\"\n            hint=\"Password is case sensitive\"\n            @click:append=\"show = !show\"\n          ></v-text-field>\n        </v-col>\n        <v-col>\n          <v-btn\n            block\n            text\n            class=\"login-btn\"\n          >\n            Log In\n          </v-btn>\n        </v-col>\n\n  </v-card>\n</template>\n\n<style>\n  .logo{\n    background-color: #855385;\n  }\n\n  .logo img{\n    width: 170px;\n    text-align: center;\n  }\n\n  .login-btn{\n    background-color: #855385 !important;\n    color: #fff !important;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}