50,\"sort\":\"ASC\",\"columnName\":\"CustomerName\",\"keyword\":\"\"};\r\n                const headers = {\r\n                    headers: {\r\n                        'Authorization': \"Bearer \" + localStorage.getItem(\"token\")\r\n                    }\r\n                };\r\n                const response = await axios.post(\"https://qwiktimeapidev.azurewebsites.net/api/Customer/ListCustomer?isDeleted=\" + listType, payloadData, headers);\r\n                console.log(response.data);\r\n\r\n                this.customerList = response.data.Response;\r\n            }\r\n            catch(e) {\r\n                console.log(e);\r\n            }\r\n        },\r\n      },\r\n      mounted() {\r\n        this.GetCustomerList(\"2\");\r\n      }\r\n    }\r\n</script>\r\n\r\n<template>\r\n    <div>\r\n        <v-breadcrumbs :items=\"items\"></v-breadcrumbs>\r\n    </div>\r\n\r\n    <v-col lg=\"12\" class=\"customer-list\">\r\n        <v-card class=\"card\">\r\n            <v-card-title>Customer List</v-card-title>\r\n            <v-card-subtitle>Tap + button at bottom to add new customer</v-card-subtitle>\r\n            <v-col lg=\"12\" class=\"p-15\">\r\n                <table class=\"table table-striped table-bordered\" id=\"customer_list\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>Customer Name</th> \r\n                            <th>Customer Type</th>\r\n                            <th>Industry</th>  \r\n                            <th>Status</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"item in this.customerList\" v-bind:key=\"item.CustomerId\">\r\n                            <td>{{item.CustomerName}}</td>\r\n                            <td>{{item.CustomerType}}</td>\r\n                            <td>{{item.Industry}}</td>\r\n                            <td>\r\n                                <span v-if=\"item.IsDeleted\" class=\"bg-danger\">Inactive</span>\r\n                                <span v-else class=\"bg-success\">Active</span>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n\r\n                <!-- <v-data-table\r\n                    :headers=\"headers\"\r\n                    :items=\"customerList\"\r\n                    :search=\"search\"\r\n                ></v-data-table>\r\n\r\n                <v-simple-table height=\"300px\" lg=\"12\">\r\n                    <template v-slot:default>\r\n                        <thead>\r\n                            <tr>\r\n                                <th class=\"text-left\">\r\n                                    Customer Name\r\n                                </th>\r\n                                <th class=\"text-left\">\r\n                                    Customer Type\r\n                                </th>\r\n                                <th class=\"text-left\">\r\n                                    Industry\r\n                                </th>\r\n                                <th class=\"text-left\">\r\n                                    Status\r\n                                </th>\r\n                            </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                            <tr\r\n                                v-for=\"item in customerList\"\r\n                                :key=\"item.name\"\r\n                            >\r\n                                <td>{{item.CustomerName}}</td>\r\n                                <td>{{item.CustomerType}}</td>\r\n                                <td>{{item.Industry}}</td>\r\n                                <td>{{item.IsDeleted}}</td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </template>\r\n                </v-simple-table> -->\r\n            </v-col>\r\n        </v-card>\r\n        <button class=\"circle-btn\" @click=\"router.push('/addcustomer')\">\r\n            <v-icon>mdi-plus</v-icon>\r\n        </button>\r\n        <div>\r\n            \r\n        </div>\r\n    </v-col>\r\n</template>\r\n\r\n<style scoped>\r\n    v-card-title{\r\n        position: sticky;\r\n    }\r\n\r\n    .card{\r\n        height: 550px;\r\n    }\r\n\r\n    .card:hover{\r\n        overflow: overlay;\r\n    }\r\n\r\n    table{\r\n        width: 100%;\r\n    }\r\n\r\n    table th{\r\n        text-align: left;\r\n        background-color: #673ab7;\r\n        color: #fff;\r\n        padding: 5px 5px 5px 10px;\r\n    }\r\n\r\n    table td{\r\n        text-align: left;\r\n        padding: 5px 5px 5px 10px;\r\n    }\r\n\r\n    tr:nth-child(even) {\r\n        background: #f5efff !important;\r\n    }\r\n\r\n    tr:hover{\r\n        background-color: #F4F6F6;\r\n    }\r\n\r\n    .bg-success{\r\n        background-color: green;\r\n        padding: 5px;\r\n        font-size: 13px;\r\n        color: #fff;\r\n        border-radius: 3px;\r\n    }\r\n\r\n    .bg-danger{\r\n        background-color: rgb(193, 5, 5);\r\n        padding: 5px;\r\n        font-size: 13px;\r\n        color: #fff;\r\n        border-radius: 3px;\r\n    }\r\n\r\n    .circle-btn{\r\n        border-radius: 30px;\r\n        background: #673ab7;\r\n        padding: 15px;\r\n        color: #fff;\r\n        bottom: 45px;\r\n        right: 25px;\r\n        position: absolute;\r\n        float: right;\r\n    }\r\n\r\n    .p-15{\r\n        padding: 15px;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}